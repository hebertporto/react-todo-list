{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION__"] }],
        "semi": ["warn", "never"],
        "no-console": 1
    },
    "settings": {
        "import/resolver": {
        "webpack": { "config": "webpack.config.js" },
        "babel-module-alias": {}
        }
  }
}
